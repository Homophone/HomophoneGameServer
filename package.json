{
  "name": "homophone-game-server",
  "version": "1.0.0",
  "engines": {
    "node": ">=6.7.0 <6.8.1"
  },
  "description": "Server for homephone app",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start-watch": "./node_modules/nodemon/bin/nodemon.js index.js",
    "db-create": "ruby ./scripts/db_create.rb",
    "test-db-create": "ENVIRONMENT=test npm run db-create",
    "db-migrate": "npm run sequelize -- db:migrate",
    "test-db-migrate": "npm run sequelize -- db:migrate --env test",
    "test": "mocha api/**/*.test.js --timeout 10000",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "sequelize": "node_modules/.bin/sequelize",
    "get-homophones": "node scripts/get-homophones",
    "pre-push": "npm run lint && npm run test",
    "setup-git-hooks": "./scripts/setup-git-hooks"
  },
  "author": "Chloe Echikson, Mark Allen",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "express-rate-limit": "^2.8.0",
    "giphy": "0.0.4",
    "graphql": "^0.10.1",
    "helmet": "^3.6.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "path": "^0.12.7",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-react-native": "^2.3.2",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0"
  }
}
